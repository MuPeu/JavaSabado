package aula7;

import javax.swing.JOptionPane;

public class Conta {
	int numero;
	String titular, sNumero, sSaldo, sLimite, sQuantidade;
	double saldo, limite, quantidade;

	boolean saca(double quantidade){
		if (this.saldo < quantidade) {
			JOptionPane.showMessageDialog(null, "Saldo Insuficiente");
			return false;
		}
		
		else{
			saldo -= quantidade;
			return true;
		}

	}
	
	void deposito(double quantidade) {
		saldo += quantidade;
		
	}
	
	void transfere(Conta destino, double valor) {
		this.saldo -= valor;
		destino.saldo += valor;
	}
	
}
